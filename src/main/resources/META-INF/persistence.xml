<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="kthfsPU" transaction-type="JTA">
    <jta-data-source>jdbc/kthfs.cloud1</jta-data-source>
    <class>se.kth.bbc.activity.ActivityDetail</class>
    <class>se.kth.bbc.activity.UserActivity</class>
    <class>se.kth.bbc.activity.UsersGroups</class>
    <class>se.kth.bbc.jobs.jobhistory.JobExecutionFile</class>
    <class>se.kth.bbc.jobs.jobhistory.JobHistory</class>
    <class>se.kth.bbc.jobs.jobhistory.JobInputFile</class>
    <class>se.kth.bbc.jobs.jobhistory.JobOutputFile</class>
    <class>se.kth.bbc.study.StudyDetail</class>
    <class>se.kth.bbc.study.StudyGroups</class>
    <class>se.kth.bbc.study.StudyTeam</class>
    <class>se.kth.bbc.study.TrackStudy</class>
    <class>se.kth.bbc.study.metadata.StudyMeta</class>
    <class>se.kth.bbc.study.samples.AnatomicalPart</class>
    <class>se.kth.bbc.study.samples.Disease</class>
    <class>se.kth.bbc.study.samples.Sample</class>
    <class>se.kth.bbc.study.samples.Samplecollection</class>
    <class>se.kth.bbc.study.services.StudyServices</class>
    <class>se.kth.kthfsdashboard.user.Username</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <shared-cache-mode>NONE</shared-cache-mode>
    <validation-mode>NONE</validation-mode>
    <properties/>
  </persistence-unit>
  <persistence-unit name="hopsPU" transaction-type="JTA">
    <jta-data-source>jdbc/hops</jta-data-source>
    <class>se.kth.bbc.study.fb.Inode</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <shared-cache-mode>NONE</shared-cache-mode>
    <validation-mode>NONE</validation-mode>
    <properties/>
  </persistence-unit>
</persistence>

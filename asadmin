// to create email
asadmin create-javamail-resource --mailhost smtp.gmail.com --mailuser suppor.biobankcloud@gmail.com --fromaddress suppor.biobankcloud@gmail.com --storeprotocol imap --storeprotocolclass com.sun.mail.imap.IMAPStore --transprotocol=smtp --transprotocolclass=com.sun.mail.smtp.SMTPTransport --debug=false --enabled=true --property "mail-smtp.starttls.enable=true:mail-smtp.socketFactory.fallback=false:mail-smtp.socketFactory.port=465:mail-smtp.socketFactory.class=javax.net.ssl.SSLSocketFactory:mail-smtp.password=XX:mail-smtp.port=465:mail-smtp.user=suppor.biobankcloud@gmail.com:mail-smtp.auth=true" mail/BBCMail
